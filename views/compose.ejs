<%- include("Partials/header") -%>


<h1>Compose</h1>
<div class="form-group">
  <label>Title</label>
  <form class="form form-group" action="/compose" method="post">
    <input class="form-control" type="text" name="title">
  </form>
  <label>Post</label>
  <form class="form form-group" action="/compose" method="post">
    <textarea class="form-control" name="name" rows="6"></textarea>
  </form>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</div>
